<p id="notice"><%= notice %></p>

<h1>Listing Services</h1>

<table>
  <thead>
    <tr>
      <th>Phase</th>
      <th>Reserved at</th>
      <th>Confirmed at datetime</th>
      <th>Cancelled at</th>
      <th>Scheduled to</th>
      <th>Requested to</th>
      <th>Started at</th>
      <th>Ended at</th>
      <th>Price double</th>
      <th>Person</th>
      <th>Person</th>
      <th>Payment type</th>
      <th>Vehicle</th>
      <th>Service type</th>
      <th>Address</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @services.each do |service| %>
      <tr>
        <td><%= service.phase %></td>
        <td><%= service.reserved_at %></td>
        <td><%= service.confirmed_at_datetime %></td>
        <td><%= service.cancelled_at %></td>
        <td><%= service.scheduled_to %></td>
        <td><%= service.requested_to %></td>
        <td><%= service.started_at %></td>
        <td><%= service.ended_at %></td>
        <td><%= service.price_double %></td>
        <td><%= service.person %></td>
        <td><%= service.person %></td>
        <td><%= service.payment_type %></td>
        <td><%= service.vehicle %></td>
        <td><%= service.service_type %></td>
        <td><%= service.address %></td>
        <td><%= service.address %></td>
        <td><%= link_to 'Show', service %></td>
        <td><%= link_to 'Edit', edit_service_path(service) %></td>
        <td><%= link_to 'Destroy', service, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Service', new_service_path %>
