<table class="table table-striped table-bordered">
  <thead>
    <tr>
    
      <th>description</th>
      <th>seats_capacity</th>
      <th>plate</th>
    </tr>
  </thead>
  <tbody>
   <% @vehicles.each do |vehicle| %>
      <tr>
       
        <td><%= vehicle.description %></td>
        <td><%= vehicle.seats_capacity %></td>
        <td><%= vehicle.plate %></td>
        
        <td><%=  @service.id %></td>
        <td><%= link_to 'Add', assign_vehicle_path(:param1 => @service.id, :param2 => vehicle.id),  { 'class' => 'btn btn-info add_vehicle_to_service'}  %>   
        </td>
      </tr>
    <% end %>
  </tbody>
</table>