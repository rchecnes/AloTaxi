<table class="table table-striped table-bordered">
  <thead>
    <tr>
      <th>Id</th>
      <th>Nombres</th>
      <th>Apellidos</th>
      <th>licencia</th>
  
    </tr>
  </thead>
  <tbody>
    <% @user.each do |user| %>
      <tr>
        <td><%= user.id %></td>
        <td><%= user.first_name%></td>
        <td><%= user.last_name%></td>
        <td><%= user.license %></td>
        <td><%=@service.id %></td> 
        <td><%= link_to 'Add', 
             assign_driver_path(:param1 => @service.id, :param2 => user.id),  
             { 'class' => 'btn btn-info add_vehicle_to_service'}  
             %>   
      </tr>
    <% end %>
  </tbody>
</table>