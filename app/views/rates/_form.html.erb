<%= simple_form_for(@rate) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :price %>
    <%= f.association :district %>
    <%= f.association :district %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
